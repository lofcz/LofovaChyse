<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DataAccess" namespace="DataAccess.Models">

  <class name="BookVersion" table="book_version">
    <id name="Id" column="id">
      <generator class="native" />
    </id>
    <property name="Text" column="text" />
    <property name="Date" column="date" />
    <property name="ChangedBy" column="changedBy" />
    <property name="Version" column="version" />
    <property name="PostId" column="postId" />
    <property name="SumText" column="sumText" />
    <property name="IsSuggestion" column="isSuggestion" />
    <property name="IsApproved" column="isApproved" />
  </class>

</hibernate-mapping>