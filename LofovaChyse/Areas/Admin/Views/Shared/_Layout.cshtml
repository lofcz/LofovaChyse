<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css">
    <link href="@Url.Content("~/Content/Buttons.css")" rel="stylesheet">
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/MicrosoftAjax.js"></script>
    <script src="~/Scripts/MicrosoftMvcAjax.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    </head>
<body>
        <nav class="navbar navbar-dark bg-dark navbar-expand-sm navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Odejít z ACP", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @{Html.RenderAction("Index", "Menu");}
                </ul>
            </div>
        </div>
    </nav>

<div class="container body-content" id="dynamicContent">
    @RenderBody()
</div>

<div class="container body-content">
    <hr />
    <footer>
        <p>&copy; LofovaChýše - CMS zítřka</p>
    </footer>
</div>

    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>